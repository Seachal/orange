<?xml version="1.0" encoding="utf-8"?>
<layout>
    <data>
        <import type="com.zhuzichu.mvvm.R"/>
        <variable name="item" type="com.zhuzichu.mvvm.viewmodel.ItemNineImageViewModel"/>
    </data>

    <androidx.cardview.widget.CardView
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            app:cardCornerRadius="3dp"
            app:onClickCommand="@{item.onClickItem}"
            android:layout_height="wrap_content">

        <com.zhuzichu.mvvm.view.layout.SquareRelativeLayout

                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    app:url="@{item.url}"
                    app:placeholderRes="@{R.drawable.ic_place_holder}"/>
        </com.zhuzichu.mvvm.view.layout.SquareRelativeLayout>
    </androidx.cardview.widget.CardView>
</layout>
